  "tokenColors": [
    // === 基础规则 ===
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#94a3b8"
      }
    },
    {
      "name": "Keyword - 主蓝加粗",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#3b82f6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator - 视觉退后",
      "scope": ["keyword.operator", "punctuation", "meta.brace"],
      "settings": {
        "foreground": "#64748b"
      }
    },
    {
      "name": "Function - 发光感",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Number, Constant - 月光黄",
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "String - 月灵花绿",
      "scope": [
        "string",
        "string.quoted.single",
        "string.quoted.double",
        "text.html.vue string"
      ],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "String Interpolation - 发光蓝",
      "scope": [
        "meta.embedded",
        "source.js.embedded",
        "string.template meta.template.expression"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Class, Type - 暖色点缀",
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "Tag - 错误红",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "text.html.vue entity.name.tag",
        "meta.tag.vue"
      ],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Attributes - 主蓝",
      "scope": [
        "entity.other.attribute-name",
        "text.html.vue entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Regular Expressions - 青色点缀",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#22d3ee"
      }
    },
    {
      "name": "URL - 下划线",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "TODO Highlight",
      "scope": ["comment.line.todo", "comment.block.todo"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#fbbf24"
      }
    },

    // === 变量分层（替换原 Variables 规则）===
    {
      "name": "Variables - 局部变量 (退后)",
      "scope": [
        "variable.other.readwrite.local",
        "variable.other.local",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },
    {
      "name": "Variables - 参数 (斜体退后)",
      "scope": ["variable.parameter", "variable.other.parameter"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cbd5e1"
      }
    },
    {
      "name": "Variables - 常量/全局 (保持亮度)",
      "scope": [
        "variable.other.constant",
        "variable.other.global",
        "variable.other.readwrite.global"
      ],
      "settings": {
        "foreground": "#e2e8f0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables - 成员属性",
      "scope": ["variable.other.property", "variable.other.member"],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },
    {
      "name": "Destructuring - 解构变量",
      "scope": ["variable.other.destructuring"],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },

    // === 键值对优化 ===
    {
      "name": "Object Keys - 普通键名 (退后)",
      "scope": [
        "meta.object.key",
        "meta.object-literal.key",
        "string.unquoted.label",
        "meta.object.key.js",
        "meta.object.key.ts"
      ],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },
    {
      "name": "Object Keys - 常量键名 (保持亮度)",
      "scope": ["meta.object.key constant", "meta.object.key constant.other"],
      "settings": {
        "foreground": "#e2e8f0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Object Keys - 嵌套键名 (更退后)",
      "scope": ["meta.object.key meta.object.key", "meta.object.key.nested"],
      "settings": {
        "foreground": "#94a3b8"
      }
    },
    {
      "name": "Key-Value Separator - 冒号 (视觉退后)",
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.separator.mapping.key-value"
      ],
      "settings": {
        "foreground": "#64748b"
      }
    },

    // === 链式编程优化 ===
    {
      "name": "Method Call - 第一层 (发光感)",
      "scope": [
        "meta.method-call entity.name.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Method Call - 链式后续 (退后)",
      "scope": [
        "meta.method-call meta.method-call entity.name.function",
        "meta.function-call meta.function-call entity.name.function",
        "meta.chain entity.name.function"
      ],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },
    {
      "name": "Method Call - 深层链式 (更退后)",
      "scope": [
        "meta.method-call meta.method-call meta.method-call entity.name.function"
      ],
      "settings": {
        "foreground": "#94a3b8"
      }
    },
    {
      "name": "Method Chain Separator - 点操作符",
      "scope": ["punctuation.accessor", "punctuation.separator.dot"],
      "settings": {
        "foreground": "#64748b"
      }
    },

    // === 数组元素 ===
    {
      "name": "Array - 元素",
      "scope": ["meta.array.element"],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },

    // === HTML / Vue 模板（保持不变）===
    {
      "name": "HTML Doctype",
      "scope": ["meta.tag.sgml.html", "entity.name.tag.doctype.html"],
      "settings": { "foreground": "#64748b" }
    },
    {
      "name": "HTML Attribute Value",
      "scope": ["string.quoted.double.html", "string.quoted.single.html"],
      "settings": { "foreground": "#34d399" }
    },
    {
      "name": "HTML/Vue Text Content",
      "scope": [
        "text.html.basic",
        "text.html.vue text",
        "text.html.derivative"
      ],
      "settings": { "foreground": "#cbd5e1" }
    },
    {
      "name": "HTML Comment",
      "scope": ["comment.block.html", "comment.html"],
      "settings": { "fontStyle": "italic", "foreground": "#94a3b8" }
    },
    {
      "name": "HTML Script Tag Content",
      "scope": ["source.js.embedded.html"],
      "settings": { "foreground": "#e2e8f0" }
    },
    {
      "name": "HTML Style Tag Content",
      "scope": ["source.css.embedded.html"],
      "settings": { "foreground": "#e2e8f0" }
    },
    {
      "name": "Vue Directives",
      "scope": [
        "entity.other.attribute-name.html.vue",
        "meta.directive.vue",
        "punctuation.definition.directive.vue"
      ],
      "settings": { "foreground": "#c084fc", "fontStyle": "bold" }
    },
    {
      "name": "Vue Interpolation",
      "scope": [
        "expression.embbeded.vue",
        "punctuation.definition.expression.vue"
      ],
      "settings": { "foreground": "#7dd3fc" }
    },
    {
      "name": "Vue Composition API",
      "scope": ["support.function.vue", "meta.function-call.vue"],
      "settings": { "foreground": "#7dd3fc" }
    },
    {
      "name": "Vue Lifecycle Hooks",
      "scope": ["support.function.vue.lifecycle"],
      "settings": { "foreground": "#c084fc", "fontStyle": "italic" }
    },

    // === CSS（保持不变）===
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css", "meta.property-name.css"],
      "settings": { "foreground": "#7dd3fc" }
    },
    {
      "name": "CSS Units",
      "scope": [
        "keyword.other.unit.css",
        "keyword.other.unit.rem.css",
        "keyword.other.unit.px.css",
        "keyword.other.unit.em.css",
        "keyword.other.unit.percent.css"
      ],
      "settings": { "foreground": "#fbbf24" }
    },
    {
      "name": "CSS Variable Definitions",
      "scope": [
        "meta.property-list.css variable",
        "source.css variable",
        "variable.css"
      ],
      "settings": { "foreground": "#7dd3fc" }
    },
    {
      "name": "CSS Variable Usage",
      "scope": [
        "meta.function.css variable",
        "support.constant.property-value.css"
      ],
      "settings": { "foreground": "#7dd3fc" }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "entity.name.tag.css"
      ],
      "settings": { "foreground": "#3b82f6" }
    },
    {
      "name": "CSS Pseudo",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": { "foreground": "#7dd3fc", "fontStyle": "italic" }
    },
    {
      "name": "CSS At-rules",
      "scope": ["keyword.control.at-rule.css", "meta.at-rule.css keyword"],
      "settings": { "foreground": "#3b82f6", "fontStyle": "bold" }
    },
    {
      "name": "CSS Numbers",
      "scope": ["constant.numeric.css"],
      "settings": { "foreground": "#fbbf24" }
    },

    // === Markdown（保持不变）===
    {
      "name": "Markdown Heading",
      "scope": ["markdown.heading", "markup.heading"],
      "settings": { "foreground": "#34d399", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": { "foreground": "#7dd3fc" }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": { "fontStyle": "italic", "foreground": "#cbd5e1" }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": { "fontStyle": "bold", "foreground": "#e2e8f0" }
    },
    {
      "name": "Markdown - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": { "foreground": "#3b82f6" }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": { "foreground": "#94a3b8" }
    },

    // === JSON（保持不变）===
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": { "foreground": "#3b82f6" }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": { "foreground": "#fbbf24" }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": { "foreground": "#f87171" }
    },
    // === Python 专用优化（包含兜底规则）===
    {
      "name": "Python Local Variables - 局部变量退后",
      "scope": ["variable.other.readwrite.python", "variable.other.python"],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },
    {
      "name": "Python Function Parameters - 参数斜体退后",
      "scope": ["variable.parameter.python"],
      "settings": {
